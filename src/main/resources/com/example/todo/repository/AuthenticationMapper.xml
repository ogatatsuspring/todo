<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.todo.repository.AuthenticationMapper">

  <select id="selectByUsername" resultType="com.example.todo.entity.Authentication">
    select username, password, authority from authentications where username = #{username}
  </select>
  
</mapper>
